{
    "manifest_version": 2,

    "version": "1.26.1",
    "name": "Invisible Voice",
    "description": "This extension censors a website",

    "permissions": [
        "tabs",
        "storage",
        "webRequest",
        "https://antisocial.club/*",
        "unlimitedStorage"
    ],

    "content_scripts": [{
	"matches": ["<all_urls>"],
        "js": ["InvisibleVoice.js"],
	"run_at": "document_start"
    }]

}
